package com.oop.geometric;

public class Point extends Geometric {
	
	private int x1;
	private int x2;
	
	Point() {
		System.out.println("Constructor Point");
	}

	public int getX1() {
		return x1;
	}

	public void setX1(int x1) {
		this.x1 = x1;
	}

	public int getX2() {
		return x2;
	}

	public void setX2(int x2) {
		this.x2 = x2;
	}
	
	
}
