package org.oop.anonimclass;

public class Main {

	public static void main(String[] args) {
		var btn = new Button() {
			public void click() {
				System.out.println("Open file");
			}
			
			public void escape() {
				System.out.println("Escape to program");
			}
		};
		btn.click();
		btn.escape();
	}

}

class Button{
	public void click() {
		System.out.println("Enter to button");
	}
}