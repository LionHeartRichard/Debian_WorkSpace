package com.oop.animals;

public class Animals {
	private int size;
	private String tip;
	private String name;

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}

	public String getTip() {
		return tip;
	}

	public void setTip(String tip) {
		this.tip = tip;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int breath() {
		System.out.println("breath animals");
		return 0;
	}

	public int run() {
		System.out.println("run animals");
		return 0;
	}

}
