package ru.oop.tasks;

public class Main {

	public static void main(String[] args) {
		
	}
}

class DataGraph{
	double[] data;
	double[] getData() {
		return new double[] {};
	}
	
	Observer[] graphs;
}

interface Observer{
	void update();
}

class LineGraph extends DataGraph implements Observer{
	DataGraph dataGraph;

	@Override
	public void update() {
		System.out.println("Graphs drawing in LINE");
		
	}
	
}

class BarGraph extends DataGraph implements Observer{
	DataGraph dataGraph;

	@Override
	public void update() {
		System.out.println("BAR_GRAPH");		
	}
}

class ChartGraph extends DataGraph implements Observer{
	DataGraph dataGraph;

	@Override
	public void update() {
		System.out.println("Graphs drawing in chart");		
	}
}