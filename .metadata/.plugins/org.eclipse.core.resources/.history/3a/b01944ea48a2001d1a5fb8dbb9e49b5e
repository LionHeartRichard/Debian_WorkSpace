package com.oop.polimorfism;

public class Main {
	public static void main(String[] args) {
		Samurai samuray = new Samurai();
		samuray.name = "Eihido";
		samuray.type = "ia-do atack";
		samuray.attack();
		
		Warriors ronin = new JapanWarriors();
		
		ronin = samuray;
		ronin.attack();
		
		ronin = new MinamotoSamuray();
		ronin.attack();
	}
}
