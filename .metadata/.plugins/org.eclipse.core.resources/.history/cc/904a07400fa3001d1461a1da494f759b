package com.oop.myinterface;

public class Triangle extends Geometric implements MathGeometric {

	private double height, base;

	public void draw() {
		System.out.println("draw triangle");
	}

	public double getSquare(double x, double y) {
		base = x;
		height = y;
		double square = (x * y) / 2;
		return square;
	}
}
